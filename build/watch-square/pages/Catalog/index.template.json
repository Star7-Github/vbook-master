{
  "type": "div",
  "class": "wrapper",
  "style": {
    "alignItems": "center",
    "flexDirection": "column",
    "justifyContent": "center"
  },
  "styleObjectId": 937831808,
  "children": [
    {
      "type": "div",
      "$shown": "isLoading",
      "bind": 2,
      "class": "loading",
      "shown": true,
      "children": [
        {
          "type": "text",
          "attr": {
            "value": "加载中..."
          },
          "class": "loading-text"
        }
      ]
    },
    {
      "type": "list",
      "$shown": "function () { return !this.isLoading && this.showRanges; }",
      "attr": {
        "scrollbar": "true",
        "title": "true"
      },
      "bind": 2,
      "class": "range-list",
      "shown": false,
      "children": [
        {
          "type": "list-item",
          "attr": {
            "type": "li"
          },
          "bind": 1,
          "events": {
            "click": "handleBack"
          },
          "children": [
            {
              "type": "div",
              "class": "vwTitleWrapper",
              "children": [
                {
                  "type": "div",
                  "class": "vwTitle",
                  "children": [
                    {
                      "type": "vw-icon",
                      "attr": {
                        "icon": "arrow-left",
                        "size": "sm"
                      },
                      "bind": 3
                    },
                    {
                      "type": "text",
                      "attr": {
                        "$value": "titleText"
                      },
                      "bind": 1
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "list-item",
          "$repeat": "ranges",
          "attr": {
            "type": "range"
          },
          "bind": 2,
          "class": "range-item",
          "events": {
            "$click": "function (evt) { return this.selectRange(this.$idx, evt); }"
          },
          "repeat": [],
          "children": [
            {
              "type": "text",
              "attr": {
                "$value": "$item.label"
              },
              "bind": 1,
              "class": "range-text"
            }
          ]
        }
      ]
    },
    {
      "type": "list",
      "$shown": "function () { return !this.isLoading && !this.showRanges; }",
      "attr": {
        "scrollbar": "true",
        "title": "true"
      },
      "bind": 2,
      "class": "chapter-list",
      "shown": false,
      "children": [
        {
          "type": "list-item",
          "attr": {
            "type": "li"
          },
          "bind": 1,
          "events": {
            "click": "handleBack"
          },
          "children": [
            {
              "type": "div",
              "class": "vwTitleWrapper",
              "children": [
                {
                  "type": "div",
                  "class": "vwTitle",
                  "children": [
                    {
                      "type": "vw-icon",
                      "attr": {
                        "icon": "arrow-left",
                        "size": "sm"
                      },
                      "bind": 3
                    },
                    {
                      "type": "text",
                      "attr": {
                        "$value": "titleText"
                      },
                      "bind": 1
                    }
                  ]
                }
              ]
            }
          ]
        },
        {
          "type": "list-item",
          "$repeat": "chapters",
          "attr": {
            "type": "chapter"
          },
          "bind": 2,
          "class": "chapter-item",
          "events": {
            "$click": "function (evt) { return this.selectChapter(this.$item.index, evt); }"
          },
          "repeat": [],
          "children": [
            {
              "type": "text",
              "$class": "function () { return ['chapter-text', this.$item.index === this.currentIndex ? 'active' : '']; }",
              "attr": {
                "$value": "$item.title"
              },
              "bind": 1
            },
            {
              "type": "text",
              "attr": {
                "value": "已缓存"
              },
              "class": "cached-tag"
            }
          ]
        }
      ]
    }
  ]
}