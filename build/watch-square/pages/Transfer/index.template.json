{
  "type": "list",
  "attr": {
    "scrollbar": "true",
    "title": "true"
  },
  "styleObjectId": 1561212216,
  "children": [
    {
      "type": "list-item",
      "attr": {
        "type": "li"
      },
      "bind": 1,
      "events": {
        "click": "goBack"
      },
      "children": [
        {
          "type": "div",
          "class": "vwTitleWrapper",
          "style": {
            "alignItems": "center",
            "flexDirection": "column",
            "height": "80px",
            "justifyContent": "flex-end",
            "width": "100%"
          },
          "children": [
            {
              "type": "div",
              "class": "vwTitle",
              "style": {
                "alignItems": "center",
                "justifyContent": "center"
              },
              "children": [
                {
                  "type": "vw-icon",
                  "attr": {
                    "icon": "arrow-left",
                    "size": "sm"
                  },
                  "bind": 3
                },
                {
                  "type": "text",
                  "attr": {
                    "value": "传书到表"
                  },
                  "style": {
                    "color": "#ffffff",
                    "fontSize": "40px",
                    "fontWeight": "bold",
                    "marginRight": "48px"
                  }
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "list-item",
      "$shown": "isLoading",
      "attr": {
        "type": "li"
      },
      "bind": 2,
      "shown": true,
      "children": [
        {
          "type": "div",
          "class": "wrapper loading",
          "children": [
            {
              "type": "vw-loading",
              "bind": 3
            },
            {
              "type": "div",
              "class": "wrapper",
              "children": [
                {
                  "type": "text",
                  "attr": {
                    "value": "正在加载..."
                  }
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "list-item",
      "$shown": "function () { return !this.isLoading && this.bookList.length === 0; }",
      "attr": {
        "type": "li"
      },
      "bind": 2,
      "shown": false,
      "children": [
        {
          "type": "div",
          "class": "wrapper empty",
          "children": [
            {
              "type": "vw-icon",
              "attr": {
                "icon": "delete"
              },
              "bind": 3
            },
            {
              "type": "div",
              "class": "wrapper",
              "children": [
                {
                  "type": "text",
                  "attr": {
                    "value": "暂无在线书籍"
                  }
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "list-item",
      "$shown": "function () { return !this.isLoading && this.isDownloading; }",
      "attr": {
        "type": "li"
      },
      "bind": 2,
      "shown": false,
      "children": [
        {
          "type": "div",
          "class": "vwSubTitle",
          "children": [
            {
              "type": "text",
              "attr": {
                "$value": "phaseText"
              },
              "bind": 1
            }
          ]
        }
      ]
    },
    {
      "type": "list-item",
      "$shown": "function () { return !this.isLoading && this.isDownloading; }",
      "attr": {
        "type": "li"
      },
      "bind": 2,
      "class": "bookItemWrapper",
      "shown": false,
      "children": [
        {
          "type": "div",
          "class": "book-item",
          "children": [
            {
              "type": "div",
              "class": "book-info",
              "children": [
                {
                  "type": "marquee",
                  "attr": {
                    "$value": "curBookName"
                  },
                  "bind": 1,
                  "class": "book-title"
                },
                {
                  "type": "text",
                  "attr": {
                    "$value": "curBookAuthor"
                  },
                  "bind": 1,
                  "class": "book-author"
                }
              ]
            },
            {
              "type": "stack",
              "class": "downloading",
              "children": [
                {
                  "type": "div",
                  "class": "progressWrapper",
                  "children": [
                    {
                      "type": "div",
                      "bind": 1,
                      "class": "progress",
                      "inlineStyle": {
                        "$height": "function () { return '' + (this.downloadProgress < 60 ? this.downloadProgress : 60) + 'px'; }",
                        "$width": "function () { return '' + this.downloadProgress + 'px'; }"
                      }
                    }
                  ]
                },
                {
                  "type": "div",
                  "class": "progressTxt",
                  "children": [
                    {
                      "type": "text",
                      "attr": {
                        "$value": "function () { return '' + this.downloadProgress + '%'; }"
                      },
                      "bind": 1
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "list-item",
      "$shown": "function () { return !this.isLoading && this.bookList.length > 0; }",
      "attr": {
        "type": "li"
      },
      "bind": 2,
      "shown": false,
      "children": [
        {
          "type": "div",
          "class": "vwSubTitle",
          "children": [
            {
              "type": "text",
              "attr": {
                "value": "在线书籍"
              }
            }
          ]
        }
      ]
    },
    {
      "type": "list-item",
      "$repeat": "bookList",
      "attr": {
        "type": "li",
        "tid": "id"
      },
      "bind": 2,
      "class": "bookItemWrapper",
      "repeat": [],
      "children": [
        {
          "type": "div",
          "class": "book-item",
          "children": [
            {
              "type": "div",
              "class": "book-info",
              "children": [
                {
                  "type": "marquee",
                  "attr": {
                    "$value": "$item.name"
                  },
                  "bind": 1,
                  "class": "book-title"
                },
                {
                  "type": "text",
                  "attr": {
                    "$value": "$item.author"
                  },
                  "bind": 1,
                  "class": "book-author"
                }
              ]
            },
            {
              "type": "div",
              "$shown": "function () { return !this.$item.isDownloaded; }",
              "bind": 2,
              "class": "download-btn",
              "events": {
                "$click": "function (evt) { return this.startDownload(this.$item, evt); }"
              },
              "shown": false,
              "children": [
                {
                  "type": "text",
                  "attr": {
                    "value": "下载"
                  },
                  "class": "btn-icon"
                }
              ]
            },
            {
              "type": "div",
              "$shown": "$item.isDownloaded",
              "bind": 2,
              "class": "downloaded-tag",
              "shown": false,
              "children": [
                {
                  "type": "text",
                  "attr": {
                    "value": "已下载"
                  },
                  "class": "btn-icon"
                }
              ]
            }
          ]
        }
      ]
    },
    {
      "type": "list-item",
      "children": [
        {
          "type": "div",
          "style": {
            "height": "140px"
          }
        }
      ]
    }
  ]
}